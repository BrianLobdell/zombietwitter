<div class = "jumbotron jumbotron-custom">
<%= render 'pages/home' %>
  
  <table>
    <thead>
      <tr>
        <th>Status</th>
        <th>Location</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% @tweets.each do |tweet| %>
        <tr>
          <td><%= tweet.status %></td>
          <td><%= tweet.location %></td>
          <% if current_user %>
          <td><%= link_to raw('<span class="glyphicon glyphicon-edit"></span>'), edit_tweet_path(tweet) %></td>
          <td><%= link_to raw('<span class="glyphicon glyphicon-trash"></span>'), tweet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          <%end%>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>
</br>
</br>
</br>
</br>
</br>
</br>
</br>
</br>
</br>
</br>
</br>
</br>
</br>
</div>